import { DEFAULT_ACTION, ASYNC_ACTION } from './constants';

export function defaultAction() {
  return {
    type: DEFAULT_ACTION,
  };
}

// async
export const asyncAction = (params) => (dispatch, getState) => {
  setTimeout(() => {
    return {
      type: ASYNC_ACTION
    };
  },2000)
}
